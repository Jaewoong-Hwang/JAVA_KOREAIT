package ch01;

public class c01SystemOut {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		
		// \n : 개행, 줄바꿈
		// \b : 백스페이스
		// \t : 탭길이( 기본 8칸) 만큼 커서 이동
		System.out.print("HELLOWORLD\n");
		System.out.print("HELLOWORLD\n");
		System.out.print("HELLOWORLD\n");
		System.out.print("HELLOWORLD\n");
		System.out.print("HELLOWORLD\n");
	}

}
